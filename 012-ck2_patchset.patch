diff -uNr linux-4.11.3-acrux/kernel/time/hrtimer.c linux-4.11.3-acrux-r1/kernel/time/hrtimer.c
--- linux-4.11.3-acrux/kernel/time/hrtimer.c	2017-05-25 10:46:45.000000000 -0300
+++ linux-4.11.3-acrux-r1/kernel/time/hrtimer.c	2017-05-26 11:26:24.591208253 -0300
@@ -1770,3 +1770,117 @@
